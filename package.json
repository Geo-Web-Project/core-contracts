    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.1",
    "dotenv": "^14.3.2",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.2",
    "graphql": "^16.3.0",
    "hardhat": "^2.5.0",
    "hardhat-abi-exporter": "^2.8.0",
